{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2018", "dom"],
    "types": ["node", "jasmine"],
    "sourceMap": true,
    "moduleResolution": "node",
    "strict": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "jsx": "react"
  },
  "include": ["./**/*"]
}
